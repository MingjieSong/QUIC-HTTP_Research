  <!-- /.list-inline list-unstyled -->

  <p class="star-rating__answer bold-text"></p>

  <p class="star-rating__text">
    <span class="star-rating__count" data-separator="." data-reviewCount="3"></span> Ratings with an average of
    <span class="star-rating__value" data-ratingValue="4.0"></span> out of 5 stars
  </p>

</div>

<!-- /.star-rating -->


</div>
<!-- /.pannacotta shop-rating -->


    </div>

    






    <!-- /.col-xs-12 -->


      <div class="col-xs-12">

