    <script>
  (function() {
    // coupon menu arrow toggle
    var navigationMobile = document.querySelector('#navigation-mobile');

    function navigationMobileArrow(e) {
      if(navigationMobile && !navigationMobile.classList.contains('in')) {
        e.target.classList.add('navbar-toggle--open');
      }
      else {
        e.target.classList.remove('navbar-toggle--open');
      }
    }

    if (document.querySelector('.navbar-toggle')) document.querySelector('.navbar-toggle').addEventListener('click', navigationMobileArrow);

    if (!document.querySelector('.head-disclaimer')) {
      document.body.classList.add("hide-disclaimer");
    }

  })();
</script>
<script src="https://activate.platform.californiatimes.com/caltimes/thirdpartyservice/Bootstrap.js" async></script>

<style>
  .show-more {
    width: 40px;
    max-width: 40px;
  }
</style>


    
<script>
(function() {
  // coupon menu arrow toggle
  var navigationMobile = document.querySelector('#navigation-mobile');
  
  function navigationMobileArrow(e) {
    if(navigationMobile && !navigationMobile.classList.contains('in')) {
      e.target.classList.add('navbar-toggle--open');
    }
    else {
      e.target.classList.remove('navbar-toggle--open');
    }
  }
  
  if (document.querySelector('.navbar-toggle')) document.querySelector('.navbar-toggle').addEventListener('click', navigationMobileArrow);
  
})();
</script>
<script src="https://activate.platform.californiatimes.com/caltimes/thirdpartyservice/Bootstrap.js" async></script>


    

