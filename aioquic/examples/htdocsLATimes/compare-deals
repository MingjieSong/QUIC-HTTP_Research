<script>

document.addEventListener('DOMContentLoaded', function () {
  if (document.querySelector('#category-subpages-list')) {
    initCategorySubpagesList('#category-subpages-list');
  }
  
  
  
  
  
});

var oldCategorySubpagesWidth = 0;
window.addEventListener('resize', function() {
  var widthChanged = false;
  var slider = document.querySelector('#category-subpages-list');

  if (slider && slider.clientWidth !== oldCategorySubpagesWidth) {
    oldCategorySubpagesWidth = slider.clientWidth;
    widthChanged = true;
  }

  if (widthChanged && slider) {
    runSlide('group-1', [...document.querySelectorAll('#category-subpages-list .item-wrapper')]);
    repaintDots('group-1', [...document.querySelectorAll('.dot')]);
  }
}, true);

function initCategorySubpagesList(subpagesListId) {
  const categorySubpagesListAll = [...document.querySelectorAll(subpagesListId + ' .item-wrapper')];
  const categorySubpagesDotList = [...document.querySelectorAll('.dot')];
  var xDown = null;
  var yDown = null;
  categorySubpagesDotList.forEach(dot => dot.addEventListener('click', () => {
    dot.id ? runSlide(dot.id, categorySubpagesListAll) : '';
    repaintDots(dot.id, categorySubpagesDotList );
  }));
  document.querySelector(subpagesListId).addEventListener('touchstart', handleTouchStart, false);
  document.querySelector(subpagesListId).addEventListener('touchmove', handleTouchMove, false);
}

function getTouches(evt){
  return evt.touches;
}

function handleTouchStart(evt) {
  const firstTouch = getTouches(evt)[0];
  xDown = firstTouch.clientX;
  yDown = firstTouch.clientY;
};

function handleTouchMove(evt) {
  const categorySubpagesListAll = [...document.querySelectorAll('#category-subpages-list .item-wrapper')];
  const categorySubpagesDotList = [...document.querySelectorAll('.dot')];
  const getActiveGroup = document.querySelector('#category-subpages-list > .active');
  const activeItemWrapperNumber = getNumber(getActiveGroup.className);
  let nextIndex = parseInt(activeItemWrapperNumber) + 1;
  let prevIndex = parseInt(activeItemWrapperNumber) - 1;

  if ( ! xDown  || ! yDown) {
    return;
  }

  var xUp = evt.touches[0].clientX;
  var yUp = evt.touches[0].clientY;
  var xDiff = xDown - xUp;
  var yDiff = yDown - yUp;

  if ( Math.abs( xDiff ) > Math.abs( yDiff ) ) {
    if ( xDiff > 0 ) {
      if ( nextIndex > categorySubpagesListAll.length ) {
        nextIndex = 1;
      }

      runSlide('wrapper-group-' + nextIndex, categorySubpagesListAll);
      repaintDots('group-' + nextIndex, categorySubpagesDotList );
    } else {
      if ( prevIndex < 1 ) {
        prevIndex = categorySubpagesListAll.length;
      }

      runSlide('wrapper-group-' + prevIndex, categorySubpagesListAll);
      repaintDots('group-' + prevIndex, categorySubpagesDotList );
    }
  } else {
    if ( yDiff > 0) {
      return;
    } else {
      return;
    }
  }
  xDown = null;
  yDown = null;
}

function getNumber(string) {
  if (string) {
    return string.match(/\d+/)[0];
  } else {
    return 1;
  }
}

function slideTrigger(items, groupName) {
  items.forEach(currentItem => {
    if (currentItem.className.includes(groupName)) {
      currentItem.classList.add('active');
    } else {
      currentItem.classList.remove('active');
    }
  });
}



var sliderWindowWidth = null;
var isSafari = navigator.userAgent.indexOf("Safari") > -1;

function splitArrayInChunks(data, chunkSize) {
  var nodeArray = Array.from(data);
  return nodeArray.reduce(function(arrayChunk, entry, index) {
    if (index % chunkSize) {
      arrayChunk[arrayChunk.length - 1].push(entry);
    } else {
      arrayChunk.push([entry]);
    }
    return arrayChunk;
  }, []);
}

function createPostsSliderGroups(data, className) {
  var sliderDom = document.createElement('div');
  sliderDom.classList.add('posts-wrap');
  sliderDom.classList.add(className);

  var sliderList = document.createElement('div');
  sliderList.classList.add('group-list');

  var sliderLoader = document.createElement('div');
  sliderLoader.classList.add('loader');

  var loaderWrap = document.createElement('div');
  loaderWrap.classList.add('loader-image-wrap');
  loaderWrap.setAttribute('title', 'Loading...');

  var loaderImage = document.createElement('img');
  loaderImage.classList.add('loader-image');
  loaderImage.src = "/compare-deals/assets/img/loading.svg";
  loaderImage.setAttribute('alt', 'Loading...');

  loaderWrap.appendChild(loaderImage);
  sliderLoader.appendChild(loaderWrap);

  for (var group = 0, l = data.length; group < l; group++) {
    var groupContainer = document.createElement('div');
    groupContainer.classList.add('group');

    

    for (var element = 0, lg = data[group].length; element < lg; element++) {
      groupContainer.classList.add('group-length-' + data[group].length);
      groupContainer.appendChild(data[group][element]);
    }
    sliderList.appendChild(groupContainer);
    sliderDom.appendChild(sliderList);
    sliderDom.appendChild(sliderLoader);
  }

  return sliderDom;
}

function checkWindowWidth(groups) {
  var defaultGroupSizes = { desktop: 3, tablet: 4, mobile: 3 };
  var groupsSizes = groups ? groups : defaultGroupSizes;
  var width = document.body.offsetWidth;
  var windowType = 'mobile';

  if (width >= 1) {
    windowType = 'mobile';
  }

  if (width >= 576) {
    windowType = 'tablet';
  }

  if (width >= 992) {
    windowType = 'desktop';
  }

  return groupsSizes[windowType];
}

function postsSlider(sliderId, array, targetElement, groups) {
  var size = checkWindowWidth(groups);
  targetElement.after(createPostsSliderGroups(splitArrayInChunks(array, size), 'group-size-' + size));
  generateSlidingElements(sliderId, targetElement.id);
}

function generateSlidingElements(sliderId, parentId) {
  var postsWrap = document.querySelector('#' + parentId + ' + .posts-wrap');
  var dotsContainer = document.querySelector(sliderId + ' .nav-dots-row');
  var viewportWidth = postsWrap.offsetWidth;
  sliderWindowWidth = postsWrap.offsetWidth;
  var groupList = postsWrap.querySelector('.group-list');
  var length = 0;

  if (dotsContainer) {
    clearSlideDots(dotsContainer);
  }

  if (groupList) {
    groupList.childElementCount ? groupList.childElementCount : 1;
    groupList.style.minWidth = length * viewportWidth + 'px';

    for (var i = 0, l = groupList.children.length; i < l; i++) {
      groupList.children[i].style.minWidth = viewportWidth + 'px';
      if (dotsContainer) {
        addSlideDot(dotsContainer);
      }
    }
  }
}

function clearSlideDots(dotsContainer) {
  var lastChild = dotsContainer.lastElementChild;
  while (lastChild) {
    dotsContainer.removeChild(lastChild);
    lastChild = dotsContainer.lastElementChild;
  }
}

function addSlideDot(dotsContainer) {
  var li = document.createElement("li");
  dotsContainer.appendChild(li);
}

function updateCurrentDot(dotsContainer, currentSlide) {
  if(dotsContainer) {
    var currentElements = dotsContainer.querySelectorAll(".is-current");
    var listElements = dotsContainer.querySelectorAll("li");
    var currentIndex = -1 + currentSlide;

    for (let i = 0, l = currentElements.length; i < l; i++) {
      currentElements[i].classList.remove('is-current');
    }

    listElements[currentIndex].classList.add('is-current');
  }
}

function smoothScrollToX(element, interval, time) {
  var currentPosition = element.scrollLeft;
  var position = currentPosition + interval;
  var start = null;
  if(time == null) time = 400;
  position = +position, time = +time;

  window.requestAnimationFrame(function step(currentTime) {
    start = !start ? currentTime : start;
    var progress = currentTime - start;
    if (currentPosition < position) {
      element.scrollTo(((position - currentPosition) * progress / time) + currentPosition, 0);
    } else {
      element.scrollTo(currentPosition - ((currentPosition - position) * progress / time), 0);
    }
    if (progress < time) {
      window.requestAnimationFrame(step);
    } else {
      element.scrollTo(position, 0);
    }
  });
}

function postsSliderNav(parentId, sliderId) {
  var left = document.querySelector(sliderId + ' .nav-row .left');
  var right = document.querySelector(sliderId + ' .nav-row .right');
  var dotsContainer = document.querySelector(sliderId + ' .nav-dots-row');
  var sliderWrapper = document.querySelector(parentId + ' + .posts-wrap');
  if (document.querySelector('#recommended-articles-posts-list')) {
    var dotsContainer = document.querySelector(sliderId + ' .nav-dots-row');
  }
  var groupList = sliderWrapper.querySelector('.group-list');
  var slideCount = 1;
  var currentSlide = 1;

  if (groupList) {
    slideCount = groupList.childElementCount;
  }

  if (left && right) {
    postSliderNavStateCheck(left, right, dotsContainer, sliderWrapper, currentSlide, slideCount);
    left.onclick = function changeContent() {
      currentSlide = currentSlide-- <= 1 ? 1 : currentSlide;
      if (isSafari) {
        smoothScrollToX(sliderWrapper, -sliderWindowWidth);
      } else {
        sliderWrapper.scrollBy({ left: -sliderWindowWidth, top: 0, behavior: 'smooth' });
      }

      postSliderNavStateCheck(left, right, dotsContainer, sliderWrapper, currentSlide, slideCount);
    };

    right.onclick = function changeContent(e) {
      currentSlide = slideCount <= currentSlide++ ? slideCount : currentSlide;

      if (isSafari) {
        smoothScrollToX(sliderWrapper, sliderWindowWidth);
      } else {
        sliderWrapper.scrollBy({ left: sliderWindowWidth, top: 0, behavior: 'smooth' });
      }
      postSliderNavStateCheck(left, right, dotsContainer, sliderWrapper, currentSlide, slideCount);
    };
  }
}

function getNextSlide(wrapper, item) {
  return wrapper.querySelector('div.group-list div.group:nth-child(' + item + ')');
}

function handleTouchPostsSliderStart(evt) {
  const firstTouch = getTouches(evt)[0];
  xDown = firstTouch.clientX;
  yDown = firstTouch.clientY;
};

function handleTouchPostsSliderMove(evt, sliderId) {
  var left = document.querySelector(sliderId + ' .nav-row .left');
  var right = document.querySelector(sliderId + ' .nav-row .right');

  if (!xDown || !yDown) {
    return;
  }

  var xUp = evt.touches[0].clientX;
  var yUp = evt.touches[0].clientY;
  var xDiff = xDown - xUp;
  var yDiff = yDown - yUp;

  if ( Math.abs( xDiff ) > Math.abs( yDiff ) ) {
    if (xDiff > 0) {
      right.onclick.apply(right);
    } else {
      left.onclick.apply(left);
    }
  } else {
    if ( yDiff > 0) {
      return;
    } else {
      return;
    }
  }

  xDown = null;
  yDown = null;
}

function resetPostsSliderScrollPosition (parentId) {
  var sliderWrapper = document.querySelector(parentId + ' + .posts-wrap');
  sliderWrapper.scrollLeft = 0;
}

function postSliderNavStateCheck(left, right, dotsContainer, sliderWrapper, currentSlide, slideCount) {
  if (currentSlide === slideCount) {
    right.classList.remove('enabled');
    right.classList.add('disabled');
  } else {
    right.classList.remove('disabled');
    right.classList.add('enabled');
  }
  if (currentSlide === 1) {
    left.classList.remove('disabled');
    left.classList.add('enabled');
    left.classList.remove('enabled');
    left.classList.add('disabled');
  } else {
    left.classList.remove('disabled');
    left.classList.add('enabled');
  }

  updateCurrentDot(dotsContainer, currentSlide);
}

var sliderTimer = null;
var postsSliders = [];
function sliderLoaderTrigger() {
  for (let i = 0, l = postsSliders.length; i < l; i++) {
    postsSliders[i].loader = document.querySelector(postsSliders[i].parentId + ' + .posts-wrap');
    resetPostsSliderScrollPosition(postsSliders[i].parentId);
    postsSliders[i].loader.classList.add('is-loading');
  }

  sliderTimer = setTimeout(function() {
    for (let i = 0, l = postsSliders.length; i < l; i++) {
      postsSliders[i].loader.remove();

      postsSlider(postsSliders[i].sliderId, postsSliders[i].postsList, postsSliders[i].postsWrap, postsSliders[i].groups);
      postsSliderNav(postsSliders[i].parentId, postsSliders[i].sliderId);
    }
  }, 2000);
}

function postsSliderInit(parentId, sliderId, groups) {
  var postsList = document.querySelectorAll(parentId + ' .article');
  var postsWrap = document.querySelector(parentId);
  const sliderTouchable = document.querySelector(sliderId);

  if (postsList && postsWrap) {
    sliderTimer = setTimeout(function() {
      postsSlider(sliderId, postsList, postsWrap, groups);
      postsSliderNav(parentId, sliderId);
    }, 0);

    var sliderObject = {
      'parentId': parentId,
      'sliderId': sliderId,
      'groups': groups,
      'postsList': postsList,
      'postsWrap': postsWrap,
      'clientWidth': postsWrap.clientWidth,
      'loader': null
    };

    postsSliders.push(sliderObject);

    sliderTouchable.addEventListener('touchstart', handleTouchPostsSliderStart, {passive: true});
    sliderTouchable.addEventListener('touchmove',(evt) => handleTouchPostsSliderMove(evt, sliderId), {passive: true});
  }
}

window.addEventListener('resize', function(event) {
  var widthChanged = false;

  for (let i = 0, l = postsSliders.length; i < l; i++) {
    if (postsSliders[i].clientWidth !== postsSliders[i].postsWrap.clientWidth) {
      postsSliders[i].clientWidth = postsSliders[i].postsWrap.clientWidth;
      widthChanged = true;
    }
  }

  if (widthChanged) {
    clearTimeout(sliderTimer);
    sliderLoaderTrigger();
  }
}, true);

document.addEventListener("DOMContentLoaded", function () {
  hideMobileMenuCategories();
  displayShowMoreBtnNavGrid();
  initTocContent();
  tableOfContentsMobileHandler();
});

function hideMobileMenuCategories() {
  const categoriesList = document.querySelectorAll(".section-list-checkbox");
  const targetedSectionList = document.querySelectorAll('.section');
  if (categoriesList) {
    categoriesList.forEach(item => {
      item.addEventListener('click' , (e) => {
        const bool = e.target.checked ? false : true;
        categoriesList.forEach((category, i) => {
          if (categoriesList[i].checked || bool) {
            targetedSectionList[i].classList.remove('disable');
            targetedSectionList[i].classList.add('enable');
          } else {
            targetedSectionList[i].classList.remove('enable');
            targetedSectionList[i].classList.add('disable')
          }
        })
      })
    })
  }
}

function displayShowMoreBtnNavGrid() {
  const viewportWidth = Math.max(document.documentElement.clientWidth || 0, window.innerWidth || 0);
  let textList = '';
  if (viewportWidth > 767 ) {
   textList = [...document.querySelectorAll("[data-info-nav-grid-desktop]")];
  } else {
    textList = [...document.querySelectorAll("[data-info-nav-grid-mobile]")];
  }

  const toggleList = [...document.querySelectorAll("[data-toggle-nav-grid]")];
  textList.forEach( (text, i) => {
    const infoSectionText = text ? text.className.split(" ")[0] : '';
    const modifier = `${infoSectionText}--full`;
    changeButtonNavGrid(text, toggleList[i], modifier)
    if (text && toggleList[i]) {
      toggleList[i].addEventListener("click", ()=> {
        text.classList.toggle(modifier);
        changeButtonNavGrid(text, toggleList[i], modifier);
      });
    }
  });
}

function updateNavGrid(text, toggle) {
  if (text && toggle) {
    if (text.scrollHeight > text.offsetHeight) {
      toggle.classList.remove('hidden');
    } else {
      toggle.classList.add('hidden');
    }
  }
}

function changeButtonNavGrid(text, toggle, modifier) {
  const moreText = '<span class="toggle_categories display">' + "+ Show More" + '</span>';
  const lessText = '<span class="toggle_categories hide">' + "- Show Less" + '</span>';
  if (text && toggle) {
    if (text.classList.contains(modifier)) {
      toggle.innerHTML = lessText;
    } else {
      updateNavGrid(text, toggle);
      toggle.innerHTML = moreText;
    }
  }
}

function initTocContent() {
  const staticTocList = document.body.offsetWidth < 1080 ? document.querySelector('.table-of-contents-content .modal-body .description ul') : document.querySelector('#cards-toc .modal-body .description ul');
  const fetchedContentForToc = document.querySelectorAll('.page-wrapper > section > h2');
  fetchedContentForToc.forEach(tocItem => {
    let link = document.createElement('a');
    let li = document.createElement('li');
    let descriptionSpan = document.createElement('span');

    descriptionSpan.className = 'description';

    let text = document.createTextNode(tocItem.textContent);

    descriptionSpan.appendChild(text)

    link.href = '#' + tocItem.getAttribute('id');

    li.appendChild(descriptionSpan);

    link.appendChild(li);

    if (staticTocList && link) {
      staticTocList.append(link);
    }
  })
}

function checkForTocTriggerContainer() {
  const transactionalArticleBody = document.querySelector('.modular-text-block');
  if (transactionalArticleBody) {
    return transactionalArticleBody;
  }
}

function checkToEnableToc() {
  const productSectionContainer = checkForTocTriggerContainer();
  const tocContainer = document.querySelector('.table-of-contents-container');

  if (tocContainer && productSectionContainer && productSectionContainer.getBoundingClientRect().top < 130) {
    tocContainer.classList.remove('mobile-disable');
    tocContainer.classList.add('mobile-enable');
  } else if (tocContainer) {
    tocContainer.classList.remove('mobile-enable');
    tocContainer.classList.add('mobile-disable');
  }
}

function enableTocContent(e) {
  const tocContent = document.querySelector('.table-of-contents-container .table-of-contents-content');
  if (e.target.checked) {
    tocContent.classList.remove('disable')
    tocContent.classList.add('enable')
  } else {
    tocContent.classList.remove('enable')
    tocContent.classList.add('disable');
    document.querySelector('#toggle-table-of-contents').checked = false;
  }
}

function tableOfContentsMobileHandler() {
  document.addEventListener("scroll", checkToEnableToc);
}
</script>

  </div>
</body>
</html>
